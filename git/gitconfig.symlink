[user]
	name = Namdrib
	email = ph0fgazfl4j1@opayq.com

[github]
	user = Namdrib

[core]
	editor = vim
	attributesfile = ~/.gitattributes

[alias]
	br = branch
	ci = commit
	co = checkout
	p = push
	st = status

[color]
	ui = auto

[fetch]
	prune = true
[push]
	default = simple
[pull]
	rebase = false

[credential]
	helper = cache

[diff "word"]
	textconv = strings
[diff "excel"]
	textconv = strings
[diff "zip"]
	textconv = unzip -c -a

[include]
	path = ~/.gitconfig_local
